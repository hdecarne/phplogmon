<?xml version="1.0" encoding="UTF-8"?>
<logmon version="1">
	<events>
		<source refname="syslog" />
		<event type="denied">
			<pattern>/^.* Shorewall\:.*\:DROP\:IN=.* .* SRC=(.*) .* PROTO=(.*) .* DPT=(.*) .*$/U</pattern>
			<hostip>0</hostip>
			<service decoder="srvbyport">2|1</service>
		</event>
	</events>
</logmon>
