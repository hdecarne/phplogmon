<?xml version="1.0" encoding="UTF-8"?>
<logmon version="1">
	<events service="ssh">
		<source refname="syslog" />
		<event type="granted">
			<pattern>/^.* sshd.*: Accepted publickey for (.*) from (.*) port .*$/U</pattern>
			<user>0</user>
			<hostip>1</hostip>
		</event>
		<event type="denied">
			<pattern>/^.* sshd.*: Invalid user (.*) from (.*)$/U</pattern>
			<user>0</user>
			<hostip>1</hostip>
		</event>
	</events>
</logmon>
