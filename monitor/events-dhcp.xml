<?xml version="1.0" encoding="UTF-8"?>
<logmon version="1">
	<events sourceid="syslog" service="dhcp">
		<event type="granted">
			<pattern>/^.* dhcpd: DHCPACK on (.*) to (.*) .*$/U</pattern>
			<hostip>0</hostip>
			<hostmac>1</hostmac>
		</event>
		<event type="granted">
			<pattern>/^.* dhcpd: DHCPACK to (.*) \((.*)\) .*$/U</pattern>
			<hostip>0</hostip>
			<hostmac>1</hostmac>
		</event>
	</events>
</logmon>
