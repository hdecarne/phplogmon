<?xml version="1.0" encoding="UTF-8"?>
<logmon version="1">
	<events service="ipsec">
		<source refname="syslog" />
		<event type="granted">
			<pattern>/^.* racoon: INFO: ISAKMP-SA established .*\[.*\]-(.*)\[.*\] spi:.*$/U</pattern>
			<hostip>0</hostip>
			<hostmac decoder="macfromip">0</hostmac>
		</event>
	</events>
</logmon>
